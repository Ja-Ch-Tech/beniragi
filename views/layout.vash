<!-- Sign In Popup
================================================== -->
<div id="sign-in-dialog" class="zoom-anim-dialog mfp-hide dialog-with-tabs">

	<!--Tabs -->
	<div class="sign-in-form">

		<ul class="popup-tabs-nav">
			<li><a href="#login">Log In</a></li>
			<li><a href="#register">Register</a></li>
		</ul>

		<div class="popup-tabs-container">

			<!-- Login -->
			<div class="popup-tab-content" id="login">
				
				<!-- Welcome Text -->
				<div class="welcome-text">
					<h3>Connexion!</h3>
					<span>Pas de compte beniragi ? <a href="#" class="register-tab">Creez un compte!</a></span>
				</div>
					
				<!-- Form -->
				<form method="post" id="login-form">
					<div id="errorLogin" style="opacity: 0; transition: opacity 0.5s ease 0s; display: none;" class="notification error closeable">
						<p></p>
						<a class="close" href="#"></a>
					</div>
					<div class="input-with-icon-left">
						<i class="icon-material-baseline-mail-outline"></i>
						<input style="background-color:#fff;border-color:#ccc;"  type="email" class="input-text with-border" name="email" id="email" placeholder="Adresse email" required/>
					</div>

					<div class="input-with-icon-left">
						<i class="icon-material-outline-lock"></i>
						<input style="background-color:#fff;border-color:#ccc;"  type="password" class="input-text with-border" name="password" id="password" placeholder="Mot de passe" required/>
					</div>
					<a href="/recuperation/mdp" class="forgot-password">Mot de passe oublié ?</a>
				</form>
				<!-- Button -->
				<button id="login-button" class="button full-width button-sliding-icon ripple-effect" type="submit" form="login-form">Se connecter <i class="icon-material-outline-arrow-right-alt"></i></button>
				
				<!-- Social Login 
				<div class="social-login-separator"><span>or</span></div>
				<div class="social-login-buttons">
					<button class="facebook-login ripple-effect"><i class="icon-brand-facebook-f"></i> Se connecter via Facebook</button>
					<button class="google-login ripple-effect"><i class="icon-brand-google-plus-g"></i> Se connecter via Google+</button>
				</div>-->

			</div>

			<!-- Register -->
			<div class="popup-tab-content" id="register">
				
				<!-- Welcome Text -->
				<div class="welcome-text">
					<h3>Créer un compte beniragi</h3>
					<p>C'est gratuit</p>
				</div>

				
				<!-- Form -->
				<form method="post" id="register-account-form">
					<div id="errorRegister" style="opacity: 0; transition: opacity 0.5s ease 0s; display: none;" class="notification error closeable">
						<p></p>
						<a class="close" href="#"></a>
					</div>
					<!-- Account Type -->
					<div id="accountTypes" class="account-type">
					</div>
					
					<div class="input-with-icon-left">
						<i class="icon-material-baseline-mail-outline"></i>
						<input style="background-color:#fff;border-color:#ccc;"  type="email" class="input-text with-border" name="email" id="emailaddress-register" placeholder="Adresse email" required/>
					</div>

					<div class="input-with-icon-left" title="Doit avoir aumoins 5 caracteres" data-tippy-placement="bottom">
						<i class="icon-material-outline-lock"></i>
						<input style="background-color:#fff;border-color:#ccc;"  min="5" type="password" class="input-text with-border" name="password" id="password-register" placeholder="Creez un mot de passe" required/>
					</div>
				</form>
				
				<!-- Button -->
				<button id="register-button" class="margin-top-10 button full-width button-sliding-icon ripple-effect" type="submit" form="register-account-form">Terminer <i class="icon-material-outline-arrow-right-alt"></i></button>
				
				<!-- Social Login 
				<div class="social-login-separator"><span>or</span></div>
				<div class="social-login-buttons">
					<button class="facebook-login ripple-effect"><i class="icon-brand-facebook-f"></i> S'inscrire via Facebook</button>
					<button class="google-login ripple-effect"><i class="icon-brand-google-plus-g"></i> S'inscrire via Google+</button>
				</div> -->

			</div>

		</div>
	</div>
</div>
